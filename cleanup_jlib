#!/bin/csh
cd cfitsio/distrib/cfitsio
make clean
cd ../../..
cd jlp_fits
make -f Makefile.jlp_fits clean
cd ../jlp_numeric
make -f Makefile.jlp_numeric clean
cd fftw3_src/fftw-3.3.4
make clean
cd ../../..
cd jlp_splot
make -f Makefile.jlp_splot clean
cd ../jlp_wxplot
make clean
# For Linux:
cd ../jlp_x11plot
make -f Makefile.jlp_x11plot clean
